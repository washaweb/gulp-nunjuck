{
  "name": "gulp-nunjucks-browserify",
  "version": "0.1.0",
  "description": "A realy simple static site builder with gulp/browserify, less/js auto-compilation, nunjucks html templates and json metadatas.",
  "author": "Olivier Kautz & Jérôme Poslednik",
  "license": "MIT",
  "contributors": [
    {
      "name": "Olivier Kautz",
      "email": "olivier@k3z.fr"
    },
    {
      "name": "Jérome Poslednik",
      "email": "webmaster@washaweb.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/washaweb/gulp-nunjuck.git"
  },
  "browserify": {
    "transform": [
      "browserify-css"
    ]
  },
  "browserify-css": {
    "autoInject": true,
    "minify": true,
    "rootDir": "."
  },
  "devDependencies": {
    "browserify": "^16.1.0",
    "browserify-css": "^0.14.0",
    "connect": "^3.3.5",
    "connect-livereload": "^0.6.0",
    "del": "^3.0.0",
    "gulp": "^3.8.10",
    "gulp-livereload": "^3.8.0",
    "gulp-browserify": "^0.5.1",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.5.2",
    "gulp-connect": "^5.5.0",
    "gulp-data": "^1.2.0",
    "gulp-less": "^4.0.0",
    "gulp-nunjucks-render": "^2.2.2",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-util": "^3.0.4",
    "gulp-watch": "^5.0.0",
    "gulp-sync": "^0.1.4",
    "less-plugin-clean-css": "^1.5.0",
    "node-fs-extra": "^0.8.1",
    "serve-static": "^1.9.2",
    "underscore": "^1.8.3",
    "underscore.string": "^3.1.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.1.1"
  },
  "dependencies": {
    "bootstrap": "3.3.4",
    "font-awesome": "^4.6.0",
    "jquery": "3.3.1"
  }
